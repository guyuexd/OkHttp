<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.okhttptest">
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>

    <application
        android:name=".LMyApplication"
        android:allowBackup="false"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >

        <activity android:name=".LMainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".activities.OkHttpDemoActivity"
            android:screenOrientation="portrait"/>
        <activity android:name=".activities.OkHttpDemoDownloadFileActivity"
            android:screenOrientation="portrait"/>
        <activity android:name=".activities.OkHttpCookieActivity"/>
        <activity android:name=".activities.OkHttpDemoUploadFileActivity"/>
    </application>
</manifest>
